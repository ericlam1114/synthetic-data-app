{
  "name": "synthetic-data-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:memory": "node --max-old-space-size=4096 --expose-gc node_modules/.bin/next dev",
    "build": "next build",
    "build:memory": "node --max-old-space-size=4096 --expose-gc node_modules/.bin/next build",
    "start": "next start",
    "start:memory": "node --max-old-space-size=4096 --expose-gc node_modules/.bin/next start",
    "lint": "next lint",
    "worker": "node --max-old-space-size=4096 --expose-gc scripts/worker.js",
    "worker:debug": "NODE_DEBUG=bull node --max-old-space-size=4096 --expose-gc scripts/worker.js",
    "dev:with-worker": "concurrently \"npm run dev\" \"npm run worker\"",
    "start:with-worker": "concurrently \"npm run start\" \"npm run worker\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.414.0",
    "@aws-sdk/client-textract": "^3.787.0",
    "@aws-sdk/lib-storage": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-radio-group": "^1.2.4",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-tooltip": "^1.2.0",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.488.0",
    "mongoose": "^8.13.2",
    "next": "15.3.0",
    "openai": "^4.93.0",
    "pdf-parse": "^1.1.1",
    "radix-ui": "^1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "redis": "^4.7.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "mongodb": "^6.15.0",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^4.1.4"
  }
}
